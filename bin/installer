#!/usr/bin/env sh
ROOT="$(pwd)"
cd "$ROOT" || exit
bin/dispatcher download --url https://download.java.net/java/GA/jdk17/0d483333a00540d886896bac774ff48b/35/GPL/openjdk-17_windows-x64_bin.zip && unzip java/GA/jdk17/0d483333a00540d886896bac774ff48b/35/GPL/openjdk-17_windows-x64_bin.zip && rm -rf java/
export JAVA_HOME "$ROOT/jdk-17"
echo OpenJDK17 has installed successfully.